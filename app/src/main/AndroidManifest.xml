<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/logo"
        android:label=""
        android:roundIcon="@mipmap/logo_rond"
        android:supportsRtl="true"
        android:theme="@style/Base.Theme.Projet_Session_Omar_Sebastien_v1"
        tools:targetApi="31">
        <activity
            android:name=".CreerCompteActivity"
            android:exported="false"
            android:label="@string/creerCompte_activity_title"
            android:parentActivityName=".HomeActivity"/>
        <activity
            android:name=".PanierActivity"
            android:exported="false"
            android:label="@string/paneier_activity_title"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".HomeActivity"
            android:exported="false"
            android:label="@string/home_activity_title" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



<!--        <uses-permission android:name="android.permission.INTERNET" />-->

    </application>

</manifest>